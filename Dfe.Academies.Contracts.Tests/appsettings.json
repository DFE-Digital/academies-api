{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft": "Information",
            "Microsoft.Hosting.Lifetime": "Information",
            "Microsoft.AspNetCore": "Warning"
        },
        "Console": {
            "FormatterName": "simple",
            "FormatterOptions": {
                "IncludeScopes": true
            }
        }
    },
    "ApplicationInsights": {
        "ConnectionString": "Copy connection string from Application Insights Resource Overview"
    },
    "AllowedHosts": "*",
    "ApiKeys": [ "{\"userName\": \"PactTest\", \"apiKey\": \"abcdef12-3456-7890-abcd-ef0123456789\"}" ],
    "ConnectionStrings": {
        "DefaultConnection": "Server=localhost,1433;Database=sip;User Id=sa;TrustServerCertificate=True;Password=Your_password123"
    },
    "SyncAcademyConversionProjectsSchedule": "0 0/15 * * * *",
    "Serilog": {
        "Using": [
            "Serilog.Sinks.ApplicationInsights"
        ],
        "MinimumLevel": {
            "Default": "Information",
            "Override": {
                "Microsoft": "Warning",
                "System": "Warning"
            }
        },
        "Enrich": [ "FromLogContext" ],
        "Properties": {
            "Application": "Dfe.Academies.Api"
        }
    }
}